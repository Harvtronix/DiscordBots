{
  "name": "discordbots",
  "version": "1.5.2",
  "description": "",
  "scripts": {
    "start": "npx pm2-runtime start pm2-apps.json",
    "format": "prettier --write \"packages/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint packages --max-warnings=0 && prettier --check \"packages/**/*.{js,jsx,ts,tsx}\"",
    "postversion": "sh scripts/postversion.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpctorr/DiscordBots.git"
  },
  "author": "vpctorr",
  "license": "MIT",
  "homepage": "https://github.com/vpctorr/DiscordBots#readme",
  "bugs": {
    "url": "https://github.com/vpctorr/DiscordBots/issues"
  },
  "workspaces": [
    "packages/MakePDF",
    "packages/VoiceNotify"
  ],
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-universe": "^10.0.0",
    "prettier": "^2.5.1"
  },
  "eslintConfig": {
    "extends": [
      "universe/node"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 120
  },
  "dependencies": {
    "pm2": "^5.1.2"
  },
  "engines": {
    "node": "16.x",
    "npm": ">=7"
  }
}
